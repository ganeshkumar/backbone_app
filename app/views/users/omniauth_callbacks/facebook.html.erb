<% content_for :javascript do %>
<script type="text/javascript">
    if(window.opener) {
        window.opener.location.href = "/#";
        window.close();
    }

    window.opener.BackboneApp.change_password("<%= current_user.sign_in_count %>", <%=  current_user.to_json.html_safe%>)
</script>
<% end %>